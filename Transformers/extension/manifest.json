{
    "manifest_version": 3,
    "name": "Hindi Tokenizer",
    "version": "1.0",
    "permissions": ["contextMenus", "activeTab", "storage", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["http://localhost:5000/*"], 
    "action": {
      "default_popup": "popup\\popup.html"
    }
  }